{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "react-json-v-2-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/reactJsonV2/ReactJsonV2WebPart.js",
          "manifest": "./src/webparts/reactJsonV2/ReactJsonV2WebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "ReactJsonView": {
      "path":"https://cdn.jsdelivr.net/npm/react-json-view@1.21.0/dist/main.min.js",
      "globalName":"ReactJsonView"
    }
  },
  "localizedResources": {
    "ReactJsonV2WebPartStrings": "lib/webparts/reactJsonV2/loc/{locale}.js"
  }
}
